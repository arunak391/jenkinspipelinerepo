pipeline
{
agent any
stages{
stage ('BUILD')
  {
agent any;
steps {
echo "This is BUILD stage"
      }
  }
stage ('TEST PARALLEL') 
{
parallel {
parameters {
  string defaultValue: 'Hostname', name: 'BHUVANENDER'
         }
stage('TEST ON CHROME') {
env currentBuild.number= 1
   agent {  label 'jenkins' } 
steps {
echo "This is test on chrome browser"
echo "Build number is ${currentBuild.number}"
echo "$Hostname"
      }
    }
    stage ('TEST ON SAFARI') {
   agent {  label 'jenkins' }
steps {
echo "This is test on safari browser"
      }
    }
    stage ('TEST ON Mozilla') {
   agent {  label 'maste' }
steps {
echo "This is test on safari browser"
      }
    }
    stage ('TEST ON internetexplorer') {
   agent {  label 'master' }
steps {
echo "This is test on safari browser"
sh 'sleep 5'
      }
    }
    }
    }
    }
    }
    }
