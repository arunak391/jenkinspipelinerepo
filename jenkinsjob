pipeline
{
agent any
parameters {
  string defaultValue: 'name', name: 'BHUVANENDER'
         }

         environment {
  Build_Num="1"
}           
stages{
stage ('BUILD')
  {
agent any;
steps {
echo "This is BUILD stage"
      }
  }
stage ('TEST PARALLEL') 
{
parallel {
stage ('TEST ON CHROME') {
agent {  label 'jenkins' } 
steps {
echo "This is test on chrome browser"
echo "Build number is ${Build_Num}"
echo "$name"
      }
    }
    stage ('TEST ON SAFARI') {
   agent {  label 'jenkins' }
steps {
echo "This is test on safari browser"
      }
    }
    stage ('TEST ON Mozilla') {
   agent {  label 'maste' }
steps {
echo "This is test on safari browser"
      }
    }
    stage ('TEST ON internetexplorer') {
   agent {  label 'master' }
steps {
echo "This is test on safari browser"

      }
    }
    }
    }
    }
    }
    
