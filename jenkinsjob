pipeline
{
agent any
parameters {
  string defaultValue: 'name', name: 'BHUVANENDER'
         }
         environment {
  Build_Num="1"
  ip="1.1.1.1"
}           
stages{
stage ('BUILD')
  {
agent any;
steps {
echo "This is BUILD stage"
      }
  }
stage ('TEST PARALLEL') 
{
parallel {
stage ('TEST ON CHROME') {
agent any
steps {
echo "This is test on chrome browser"
echo "Build number is ${Build_Num}"
echo "${name}"
echo "${ip}"
echo "ifconfig"
//Jenkins.getInstance().getComputer(env['master']).getHostName()


      }
    } 
    }
    }
    }
    }
    
